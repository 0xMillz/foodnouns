{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "composite": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "noEmit": false,
    "esModuleInterop": true
  },
  "include": [
      "./src/**/*.ts",
      "./src/**/*.tsx",
    "./declarations.d.ts"
    ],
    "exclude": ["node_modules", "build", "dist"],
//  "references": [
//    {
//      "path": "../foodnouns-contracts/tsconfig.build.json"
//    }
//  ]
}
